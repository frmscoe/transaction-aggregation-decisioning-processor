{
  "name": "transaction-agreggation-decisioning-processor",
  "version": "1.2.0",
  "description": "transaction-agreggation-decisioning-processor",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "nodemon -r dotenv/config",
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "prepare": "husky install",
    "test": "jest --config=jest.config.ts --passWithNoTests --forceExit",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Aaron Reynoza",
      "email": "ldaaron38@gmail.com"
    },
    {
      "name": "Joey Goksu",
      "email": "me@joeygoksu.com"
    },
    {
      "name": "Theophilus Okwugwuni",
      "email": "theophilusdev@gmail.com"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "^4.0.0-rc.5",
    "@frmscoe/frms-coe-startup-lib": "^2.2.0-alpha.1",
    "@log4js-node/logstash-http": "^1.1.0",
    "arangojs": "^8.6.0",
    "dotenv": "^16.3.1",
    "fast-json-stringify": "^5.8.0",
    "log4js": "^6.4.0",
    "tslib": "^2.6.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/arangodb": "^3.5.11",
    "@types/jest": "^29.5.11",
    "@types/log4js": "^2.3.5",
    "@types/node": "^20.11.5",
    "@types/nodemon": "^1.19.1",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "husky": "8.0.3",
    "ioredis-mock": "^8.8.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
